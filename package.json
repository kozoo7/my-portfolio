{
  "name": "noah-portfolio",
  "version": "1.0.0",
  "description": "Noah's Creative Portfolio Website",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "build": "npm run optimize-images && npm run minify-css && npm run minify-js",
    "optimize-images": "imagemin images/* --out-dir=dist/images",
    "minify-css": "cleancss -o dist/css/style.min.css css/*.css",
    "minify-js": "uglifyjs js/*.js -o dist/js/bundle.min.js",
    "lint": "eslint js/*.js server/*.js",
    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.55.0",
    "imagemin-cli": "^7.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "uglify-js": "^3.17.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kozoo7/my-portfolio.git"
  },
  "keywords": [
    "portfolio",
    "creative",
    "design",
    "motion graphics",
    "vfx"
  ],
  "author": "Noah",
  "license": "MIT"
}